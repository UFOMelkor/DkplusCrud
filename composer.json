{
    "name": "dkplus/crud",
    "description": "CRUD Controller for zf2-modules",
    "type": "library",
    "keywords": ["zf2"],
    "homepage": "https://github.com/UFOMelkor/DkplusCrud",
    "license": "MIT",

    "require": {

        "zendframework/zend-eventmanager": ">=2.1",
        "zendframework/zend-form": ">=2.1",
        "zendframework/zend-modulemanager": ">=2.1",
        "zendframework/zend-mvc": ">=2.1",
        "zendframework/zend-paginator": ">=2.1",
        "zendframework/zend-session": ">=2.1",
        "zendframework/zend-stdlib": ">=2.1.2",
        "zendframework/zend-view": ">=2.1",

        "dkplus/base": ">=0.2.0",

        "zf-commons/zfc-base": ">=0.1.0"
    },

    "require-dev": {
        "doctrine/orm": "2.*",
        "doctrine/doctrine-orm-module": ">=0.7"
    },

    "suggest": {
        "doctrine/orm": "if you want to use doctrine orm as backend.",
        "doctrine/doctrine-orm-module": "if you want to use doctrine orm as backend."
    },

    "autoload": {
        "psr-0": {
            "DkplusCrud": "src/"
        }
    }
}
